{
  "name": "preact-typescript-parcel-template",
  "version": "1.0.0-alpha",
  "description": "Preact TypeScript Parcel Template",
  "author": "Mark <mark@remarkablemark.org>",
  "source": "public/index.html",
  "scripts": {
    "build": "parcel build",
    "clean": "rm -rf .parcel-cache dist",
    "lint": "eslint .",
    "lint:fix": "npm run lint -- --fix",
    "prepare": "husky",
    "start": "parcel --open",
    "tsc": "tsc --noEmit"
  },
  "homepage": "https://remarkablemark.org/preact-typescript-parcel-template/",
  "devDependencies": {
    "@commitlint/cli": "19.8.1",
    "@commitlint/config-conventional": "19.8.1",
    "@eslint/compat": "1.2.9",
    "@eslint/eslintrc": "3.3.1",
    "@eslint/js": "9.27.0",
    "@parcel/packager-raw-url": "2.15.1",
    "@parcel/transformer-webmanifest": "2.15.1",
    "@typescript-eslint/eslint-plugin": "8.32.1",
    "@typescript-eslint/parser": "8.32.1",
    "eslint": "9.26.0",
    "eslint-plugin-prettier": "5.4.0",
    "husky": "9.1.7",
    "lint-staged": "16.0.0",
    "parcel": "2.15.1",
    "preact": "10.26.6",
    "prettier": "3.5.3",
    "typescript": "5.8.3"
  },
  "private": true,
  "license": "MIT",
  "alias": {
    "preact/jsx-dev-runtime": "preact/jsx-runtime"
  }
}
