{
  "name": "preact-typescript-parcel-template",
  "version": "1.0.0-alpha",
  "description": "Preact TypeScript Parcel Template",
  "author": "Mark <mark@remarkablemark.org>",
  "source": "public/index.html",
  "scripts": {
    "build": "parcel build",
    "clean": "rm -rf .parcel-cache dist",
    "lint": "eslint .",
    "lint:fix": "npm run lint -- --fix",
    "prepare": "husky",
    "start": "parcel --open",
    "tsc": "tsc --noEmit"
  },
  "homepage": "https://remarkablemark.org/preact-typescript-parcel-template/",
  "devDependencies": {
    "@commitlint/cli": "19.6.1",
    "@commitlint/config-conventional": "19.6.0",
    "@eslint/compat": "1.2.4",
    "@eslint/eslintrc": "3.2.0",
    "@eslint/js": "9.17.0",
    "@parcel/packager-raw-url": "2.13.2",
    "@parcel/transformer-webmanifest": "2.13.2",
    "@typescript-eslint/eslint-plugin": "8.18.0",
    "@typescript-eslint/parser": "8.18.0",
    "eslint": "9.17.0",
    "eslint-plugin-prettier": "5.2.1",
    "husky": "9.1.7",
    "lint-staged": "15.2.11",
    "parcel": "2.13.2",
    "preact": "10.25.2",
    "prettier": "3.4.2",
    "typescript": "5.7.2"
  },
  "private": true,
  "license": "MIT",
  "alias": {
    "preact/jsx-dev-runtime": "preact/jsx-runtime"
  }
}
