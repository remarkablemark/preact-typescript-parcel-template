{
  "name": "preact-typescript-parcel-template",
  "version": "1.0.0-alpha",
  "description": "Preact TypeScript Parcel Template",
  "author": "Mark <mark@remarkablemark.org>",
  "source": "public/index.html",
  "scripts": {
    "build": "parcel build",
    "clean": "rm -rf .parcel-cache dist",
    "lint": "eslint --ignore-path .gitignore .",
    "lint:fix": "npm run lint -- --fix",
    "postinstall": "husky install",
    "start": "parcel --open",
    "tsc": "tsc --noEmit"
  },
  "homepage": "https://remarkablemark.org/preact-typescript-parcel-template/",
  "devDependencies": {
    "@commitlint/cli": "17.4.2",
    "@commitlint/config-conventional": "17.4.2",
    "@parcel/packager-raw-url": "2.8.2",
    "@parcel/transformer-webmanifest": "2.8.2",
    "@typescript-eslint/eslint-plugin": "5.57.1",
    "@typescript-eslint/parser": "5.48.2",
    "eslint": "8.32.0",
    "eslint-plugin-prettier": "4.2.1",
    "husky": "8.0.3",
    "lint-staged": "13.1.0",
    "parcel": "2.8.2",
    "preact": "10.11.3",
    "prettier": "2.8.3",
    "typescript": "4.9.4"
  },
  "private": true,
  "license": "MIT",
  "alias": {
    "preact/jsx-dev-runtime": "preact/jsx-runtime"
  }
}
